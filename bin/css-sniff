#!/usr/bin/env php
<?php
declare(strict_types=1);

require __DIR__ . '/../vendor/autoload.php';

$sniff_command = new \Hostnet\Component\CssSniff\Command\SniffCommand();

$application = new \Symfony\Component\Console\Application();
